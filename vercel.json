{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": [
          "dist/apps/api/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/src/main.ts"
    }
  ],
  "installCommand": "npm install --loglevel verbose",
  "buildCommand": "npx nx build api --prod",
  "outputDirectory": "dist/apps/api"
}
